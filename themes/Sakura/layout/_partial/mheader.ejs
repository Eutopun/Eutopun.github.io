<div id="mo-nav" class="">
  <div class="m-avatar">
    <img src="<%- (theme.cdn || '') + theme.avatar%>">
  </div>
  <p style="text-align: center; color: #504b4d; font-weight: 900; font-family: 'Caleigh'; font-size: 23px; letter-spacing: 1.5px"><%- theme.prefixName%><%- theme.siteName%></p>
  <p style="text-align: center; word-spacing: 20px; padding-right: 17px;">
    <% if (theme.msocial) {%>
      <% for (var i in theme.msocial) {%>
        <a href="<%- theme.msocial[i].url%>" class="fa <%- theme.msocial[i].fa%>" target="_blank" style="color: #<%- theme.msocial[i].color%>; margin-left:20px"></a>
      <% } %>
    <% } %>
  </p>
  <ul id="menu-new-1" class="menu">
    <% for (var menu in theme.menus){ %>
      <li>
        <a href="<%- url_for(theme.menus[menu].path) %>" class="removeStyle">
          <span class="faa-parent animated-hover">
            <i class="fa  <%= theme.menus[menu].fa %>" aria-hidden="true"></i>
            <%= menu %>
          </span>
        </a>
        <% if (theme.menus[menu].submenus) { %>
          <ul class="sub-menu">
            <% for (var submenu in theme.menus[menu].submenus){ %>
              <li>
                <a href="<%- url_for(theme.menus[menu].submenus[submenu].path) %>" class="removeStyle faa-parent animated-hover">
                  <i class="fa <%= theme.menus[menu].submenus[submenu].fa %>" aria-hidden="true"></i>
                  <%= submenu %>
                </a>
              </li>
            <% } %>
          </ul>
        <% } %>
      </li>
    <% } %>
  </ul>
  <script>
    $('.removeStyle').on('click', function(){
      if ($('#main-container').hasClass('open')) {
        $('.openNav').removeAttr('style')
      }
    })
  </script>
  <p style="text-align: center; font-size: 20px; color: #b9b9b9; font-family: 'XinYeNianTi-2';"><%= theme.meetion %></p>
</div>
<button onclick="topFunction()" class="mobile-cd-top" id="moblieGoTop" title="Go to top" style="display: none;"><i class="fa fa-chevron-up" aria-hidden="true"></i></button>