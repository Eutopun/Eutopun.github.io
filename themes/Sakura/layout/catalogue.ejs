<div class="pattern-center-blank"></div>
<% if (page.photos && page.photos.length){ %>
  <div class="pattern-center ">
    <div class="pattern-attachment-img">
      <img src="<%- theme.lazyloadImg%>" data-src="<%= page.photos %>" class="lazyload" onerror="imgError(this,3)" style="width: 100%; height: 100%; object-fit: cover; pointer-events: none;">
    </div>
    <header class="pattern-header ">
      <h1 class="entry-title">
      <%= page.keywords %></h1>
    </header>
  </div>
<% } %>
<div id="content" class="site-content">
  <article class="post-item post-301 page type-page status-publish has-post-thumbnail hentry">
    <%- partial('_partial/category-cloud') %>
    <%- partial('_partial/tag-cloud') %>
    <!-- .entry-content -->
    <!-- <div style="text-align:center; width: 100%" class="social-share share-mobile" data-disabled="diandian, tencent"></div> -->
  <!-- .entry-footer -->
  </article>
    <!-- #post-## -->
    <section class="author-profile">
      <div class="info" itemprop="author" itemscope="" itemtype="https://schema.org/Person">
        <a href="/about/" class="profile gravatar"><img src="<%- (theme.cdn || '') + theme.avatar%>" itemprop="image" alt="<%- theme.siteName %>" height="70" width="70"></a>
        <div class="meta">
          <span class="title">Author</span>
          <h3 itemprop="name">
          <a href="<%- theme.url%>" itemprop="url" rel="author"><%- theme.siteName %></a>
          </h3>
        </div>
      </div>
      <hr>
      <p><i class="iconfont icon-write"></i><%- theme.farewell %></p>
    </section>
</div><!-- #primary -->
</div>